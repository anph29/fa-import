/* popup menu data root */
.popup-menu-data-root ul { list-style: none; margin: 0; } 
.popup-menu-data-root ul.parent { padding: 6px 0 6px 0; width: 270px; position: absolute; background-color: var(--dusk-three); z-index: 1; top: 37px; border-radius: 3px; -moz-border-radius: 3px; -webkit-border-radius: 3px; box-shadow:0 0 15px 0 var(--battleship-grey-two); } 
.popup-menu-data-root ul.parent:after, .popup-menu-data-root ul.parent:before { bottom: 100%; left: 20px; border: solid transparent; content: " "; position: absolute; } 
.popup-menu-data-root ul.parent:after { border-bottom-color: var(--dusk-three); border-width: 7px; margin-left: -10px; } 
.popup-menu-data-root ul.parent li { padding: 8px; position: relative; } 
.popup-menu-data-root ul.parent li:hover { background-color: var(--dusk-two); } 
.popup-menu-data-root ul.parent li:hover ul.sub-menu-data-root { display: block; } 
.popup-menu-data-root ul.parent li:hover:last-child ul.sub-menu-data-root:after { display: block; top:60%; } 
.popup-menu-data-root ul.parent li:hover:last-child ul.sub-menu-data-root { top:-108px; bottom: -48px; } 
.popup-menu-data-root ul.parent li span.ic-menu:before, span.ic-sub-menu:before { color: var(--white-three); } 
.popup-menu-data-root ul.parent li span.ic-menu { font-size: 16px; position: relative; top: 3px; margin-right: 10px; } 
.popup-menu-data-root ul.parent li span.ic-sub-menu { float: right; position: relative; font-size: 5px; top: 10px; right: 8px; } 
.popup-menu-data-root ul.sub-menu-data-root:after, .popup-menu-data-root ul.sub-menu-data-root:before { right: 100%; top: 50%; border: solid transparent; content: " "; position: absolute; } 
.popup-menu-data-root ul.sub-menu-data-root:after { border-right-color: var(--white-three); border-width: 12px; position: absolute; top:12px; } 
.popup-menu-data-root ul.parent li a { font-size: 13px; font-weight: 600; text-transform: uppercase; color: var(--white-three); padding: 6px 6px 6px 12px; display: block; cursor: pointer;} 
.popup-menu-data-root ul.sub-menu-data-root { display: none; width: 215px; height: auto; position: absolute; left: 267px; z-index: 2; top:0; padding: 0; background-color: var(--white-three); border-radius: 3px; -moz-border-radius: 3px; -webkit-border-radius: 3px; box-shadow:0 0 10px 0 var(--battleship-grey-two); } 
.popup-menu-data-root ul.sub-menu-data-root li { padding: 8px; border-bottom: solid 1px var(--light-blue-grey); } 
.popup-menu-data-root ul.sub-menu-data-root li:last-child { border-bottom: none; } 
.popup-menu-data-root ul.sub-menu-data-root li:hover { background-color: var(--pale-grey-six); } 
.popup-menu-data-root ul.sub-menu-data-root a:hover { color: var(--dusk); } 
.popup-menu-data-root ul.sub-menu-data-root li a { padding: 8px; color: var(--bluey-grey); } 

