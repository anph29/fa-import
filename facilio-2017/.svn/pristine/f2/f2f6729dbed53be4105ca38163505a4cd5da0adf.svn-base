/******************************************************************************/
/************************************DROPDOWN**********************************/
/******************************************************************************/
.qb-dropdown { width:158px; height:30px; border-radius:3px; border:solid 1px var(--light-blue-grey); background-color:var(--pale-grey-two); margin-left: 10px; font-size: 12px; text-align: left; padding-left: 8px; color:var(--slate); cursor: pointer; position: relative; white-space: nowrap; overflow: hidden; text-overflow: ellipsis; } 
.qb-dropdown::after { float: right; margin: 7px 5px 0 0; position: absolute; right: 0; } 
.qb-dropdown-item { font-size: 12px; } 
.qb-dropdown-item.active { background-color: var(--azure); color:var(--white-three); } 
.qb-dropdown:focus { outline: none; } 
.dropdown-menu { max-height: 260px; overflow-y: auto; } 
.dropdown-menu.show-calendar { max-height: 330px; max-width: 511px; overflow-y: auto; } 
