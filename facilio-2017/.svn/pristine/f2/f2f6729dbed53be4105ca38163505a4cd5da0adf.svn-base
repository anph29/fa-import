/* Style Data Table */
table.tb-data { width: 2200px; color: var(--slate); table-layout: fixed; } 
table.tb-data thead { background-color:var(--bright-blue); color:var(--white-three); font-size:13px; font-weight:600; text-align:center; height:65px; border-top:6px solid var(--bright-blue); border-left:2px solid var(--bright-blue); border-bottom:4px solid var(--bright-blue); } 
table.tb-data thead td { vertical-align:middle; border-left:1px solid var(--light-blue-grey); padding-top:19px; padding-bottom:16px; text-transform: uppercase; } 
table.tb-data tbody tr { border-bottom:2px solid var(--light-blue-grey); } 
table.tb-data thead .td1 { width:3%; } 
