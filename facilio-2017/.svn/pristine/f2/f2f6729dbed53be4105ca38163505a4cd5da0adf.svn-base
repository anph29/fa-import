/* Style Table */
.tb-content { overflow-x: auto; overflow-y: hidden; width: auto; margin-bottom: 10px; } 
button.btn-filter { position: absolute; height: 50px; background-color: var(--seafoam-blue); border: none; width: 40px; right: 15px; z-index: 1; cursor: pointer; top:161px; } 
button.btn-filter span.faci-dot::before { color: var(--white-three); } 
.tb-content button.btn-filter:focus { outline: none; } 
.item-filter { width: 215px; height: auto; border-radius: 3px; background-color: var(--white-three); box-shadow: 0 2px 13px 0 rgba(0, 0, 0, 0.15); font-size: 12px; position: absolute; right:15px; top: 181px; z-index: 1; padding: 7px 16px 16px 16px; } 
.item-filter ul { list-style: none; margin: 0; padding: 0; } 
.item-filter ul li label.custom-checkbox { position: relative; top:6px; } 
.item-filter ul li label.custom-checkbox:before { content: ""; position: absolute; background: transparent; padding: 22px 0 0 150px; left: 0; } 
.item-filter ul li label.custom-checkbox .custom-control-input:checked~.custom-control-indicator::before { background: var(--seafoam-blue); } 
.item-filter ul li span.sp-title { color: var(--charcoal-grey-two); } 
table.qb-table { width: 100%; color: var(--slate); table-layout: fixed; } 
table.qb-table thead tr { height: 50px; } 
table.qb-table thead tr th:not(:last-child)::after { content: ""; height: 70%; width: 1px; background: var(--light-blue-grey); position: absolute; right: -2px; top: 15%; } 
table.qb-table thead tr th:last-child::after { background: #fff; border-spacing: 50px 0; } 
table.qb-table tr.header-table th { border-bottom: none; padding: 5px; } 
table.qb-table tr:last-child { border-bottom: solid 1px var(--light-blue-grey); } 
table.qb-table tr.header-table { font-size:13px; color:var(--white-three); background-color:var(--bright-blue); border-bottom: none; } 
table.qb-table tr.header-table th { position:relative; font-weight:600 !important; text-align:center; vertical-align:middle; text-transform: uppercase; } 
