.userModal .user-content .report-body .item-2 ul{margin-top:16px;}
.userModal .user-content .report-body .item-2 li{margin-bottom:11px;}
.userModal .user-content .report-body .item-2 li span{padding-left: 10px;}
.userModal .user-content .report-body .item-2 .device{margin-left:58px;}
.userModal .user-content .report-body .prio{padding-left:0;margin-left:-22px;}
.userModal .user-content .report-body .status{padding-left:0;margin-left:-2px;}
.userModal .user-content .report-body .kind{padding-left:0;margin-left:-47px;}
.userModal .user-content .report-footer{float:right;padding-top:31px;font-size:14px;}
.userModal .user-content .report-body ul{list-style-type:none;padding:0}
