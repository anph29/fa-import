.permision-list{width:1210px;height:1028px;border-radius:3px;background-color:var(--white-three);box-shadow:0 -1px 20px 0 rgba(90, 104, 122, 0.3);margin:auto;}
.permision-list .header{padding-top:31px;}
.permision-list .header .title-header{padding-left:24px;padding-top:10px;font-size:18px;font-weight:600;text-align:left;color:var(--azure);}
.permision-list .header .btn-close{float:right;padding-right:31px;}
.permision-list .body-content{margin-top:27px;}
.permision-list .body-content .nav-search{border-bottom:solid 1px var(--light-blue-grey);margin-left:48px;padding-left:893px}
.permision-list .body-content .nav-search input{width:212px;height:30px;background-color:var(--pale-grey-two);margin-right:12px;margin-bottom:15px;}
.permision-list .body-content .nav-search .faci-search .path2:hover:before{color:var(--azure);cursor: pointer;}
.permision-list ul{list-style-type:none;padding:0}
.permision-list ul li{border-bottom:solid 1px var(--light-blue-grey);padding:16px 20px;font-size:13px;}
.permision-list ul li.active{color:var(--azure);font-weight:600;}
.permision-list ul li:hover{background-color:var(--pale-grey-five);}
.permision-list .faci-close:before{color:var(--black);}
.permision-list ul li .item{float:right;font-size:16px;}
.permision-list ul li .item a:hover:before{color:var(--azure);cursor:pointer;}
.permision-list ul li .item .faci-edit{padding-right:22px;}
