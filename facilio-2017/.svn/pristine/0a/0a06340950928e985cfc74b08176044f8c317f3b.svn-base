/******************HỘP THOẠI DIALOG****************/
/********************************************************/
/********************************************************/
/*.reportModal,.updateModal{position:fixed;z-index:1;padding-top:100px;left:0;top:0;width:100%;height:100%;overflow:auto;}*/
.report-content,.update-content{position:relative;background-color:var(--white-three);border-radius:3px;margin:auto;padding:0;box-shadow:0 -1px 20px 0 rgba(0, 0, 0, 0.3);-webkit-animation-name:animatetop;-webkit-animation-duration:0.4s;animation-name:animatetop;animation-duration:0.4s;width:415px;height:200px;}
@-webkit-keyframes animatetop{from{top:-300px;opacity:0} to{top:0;opacity:1} }
@keyframes animatetop{from{top:-300px;opacity:0} to{top:0;opacity:1} }
.report-header,.update-header{padding:11px 14px;background-color:var(--cerulean);}
.report-header .icon-votes,.update-header .icon-votes{font-size:23px;padding-right:18px;}
.report-header .qb-close,.update-header .qb-close{padding: 3px 2px;}
.report-body{float:left;}
.report-body .title-date,.update-body .title-time{padding-top:38px;float:left;padding-left:58px;color:#3f4f63;font-weight:600;font-size:14px;}
.report-footer,.update-footer{padding-top:37px;}
.report-content .report-body .qb-daterange{padding-top: 31px;padding-left: 18px}
.update-content .update-body .time{float:left;margin-top:30px;margin-left:13px;border-radius:3px;border:solid 1px var(--white);width:263px;height:35px;}
.update-content .update-body .time:focus{border:solid 1px var(--slate);outline:none;}

