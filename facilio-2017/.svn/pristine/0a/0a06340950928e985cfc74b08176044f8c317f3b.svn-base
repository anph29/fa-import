.userModal .user-content .report-body .item-1 input:focus{background-color:var(--white-three);}
.userModal .user-content .report-body .title-list{padding-top:8px;text-align:right;}
.userModal .user-content .report-body .list-la .dropdown-toggle,
.userModal .user-content .report-body .list-time .dropdown-toggle{width:275px;height:35px;border-radius:3px;border:solid 1px var(--light-blue-grey);color:var(--slate);text-align:left;font-size:14px;}
.userModal .user-content .report-body .list-time .dropdown-toggle{background-color: var(--pale-grey-four);}
.userModal .user-content .report-body .list-la .dropdown-toggle:after,
.userModal .user-content .report-body .list-time .dropdown-toggle:after{margin-left:13.5em;}
.userModal .user-content .report-body .password{text-align:left;padding-left:11px;padding-top:17px;font-size:13px;font-weight:600;letter-spacing:-0.4px;text-align:left;color:var(--azure)!important;float:left}
.userModal .user-content .report-body .title-checkbox{padding-left:21px;padding-top:18px;border-top:solid 1px var(--light-blue-grey);margin:20px 37px 0 22px;font-size:14px;font-weight:600;letter-spacing:-0.4px;color:var(--slate);}
.userModal .user-content .report-body .item-2 a{font-weight:600;letter-spacing:-0.4px;text-align:left;color:var(--slate);}
