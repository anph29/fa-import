.passModal ul{list-style-type:none;padding:0}
.passModal .change-content{margin:auto;width:553px;height:306px;border-radius:3px;background-color:var(--white-three);box-shadow:0 -1px 20px 0 rgba(90, 104, 122, 0.3);}
.passModal .change-content .change-header{padding:11px 14px;background-color:var(--cerulean);}
.passModal .change-content .change-header .icon-update{font-size:23px;padding-right:18px;float: left}
.passModal .change-content .change-header .icon-update:before{color:var(--white-three);}
.passModal .change-content .change-header .title-change{float: left;padding-top: 3px}
.passModal .change-content .change-body{padding-top:30px;}
.passModal .change-content .change-body .item-1{text-align:right;padding:0;line-height:38px;margin-left:51px;}
.passModal .change-content .change-body .item-1 ul li{padding-top:4px;font-size:13px;letter-spacing:-0.4px;text-align:right;color:var(--slate);}
.passModal .change-content .change-body .item-2{padding:0 16px;}
.passModal .change-content .change-footer{padding-right:37px;padding-top:18px;}
