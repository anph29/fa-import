 @import "root.css"; @import "util.css"; /* Style Root Body */
 body { background-color:var(--white-three); font-family:'Open Sans' !important; } 
 body a:hover { text-decoration: none; } 
 body .faci-search:hover span.path2::before { color:var(--azure); } 
 /* Rotate Icon Arrow */
 .rotate-arrow-up::before { display: inline-block; -webkit-transform: rotate(180deg); -moz-transform: rotate(180deg); -o-transform: rotate(180deg); -ms-transform: rotate(180deg); transform: rotate(180deg); position: relative; bottom: 2px; } 
 /* End of Style Root Body */
 
 /* Custom Checkbox And Radio */
 span.rdo-custom { width:16px; height:16px; border: solid 2px var(--light-blue-grey); background-color: #ffffff; } 
