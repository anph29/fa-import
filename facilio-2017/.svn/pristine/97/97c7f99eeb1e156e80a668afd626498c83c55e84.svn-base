 }
 
 /* style checkbox repair */
 .box-checkbox-rp { width: 318px; height:46px; border-radius:8px; background-color:#f1f1f7; padding: 8px 8px 0 8px; margin-left: 7px; } 
 .custom-cb-repair { position:relative; cursor:pointer; font-size:12px; -webkit-user-select:none; -moz-user-select:none; -ms-user-select:none; user-select:none; } 
 .custom-cb-repair input { position:absolute; opacity:0; } 
 .checkmark { position:absolute; top:-9px; left:0; height:18px; width:18px; border-radius:3px; background-color:var(--white-three); border:1px solid var(--light-blue-grey); } 
 .custom-cb-repair:hover input ~ .checkmark { background-color:var(--white-three); border:1px solid var(--light-blue-grey); } 
 .custom-cb-repair input:checked ~ .checkmark { background-color:var(--white-three); border:1px solid var(--light-blue-grey); } 
 .checkmark:after { content:""; position:absolute; display:none; } 
 .custom-cb-repair input:checked ~ .checkmark:after { display:block; } 
 .custom-cb-repair .checkmark:after { left:6px; top:3px; width:5px; height:10px; border:solid var(--dark-sage); border-width:0 2px 2px 0; -webkit-transform:rotate(45deg); -ms-transform:rotate(45deg); transform:rotate(45deg); } 
 .box-checkbox-rp.hidden-big-repair { width: 136px; background-color: transparent; } 
 .box-checkbox-rp.hidden-big-repair .form-inline { padding-top: 7px; } 
 
 
 /* style section-content*/
 .section-content { width:100%; min-height:177px; padding:21px 21px 16px 16px; border-radius:3px; margin-bottom:18px; background-color:var(--white-three); border:solid 1px var(--light-blue-grey); position: relative; } 
 .section-content:hover:before { position:absolute; content: ''; top:0; right:0; bottom:0; left:0; box-shadow: 0 0 8px 0 rgba(176, 204, 255, 0.75); } 
 .section-content.active { box-shadow: inset 6px 0 0 var(--azure); } 
